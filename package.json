  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^als/auth-manager(|/.*)$": "<rootDir>/libs/auth-manager/src/$1",
      "^als/building-block(|/.*)$": "<rootDir>/libs/building-block/$1",
      "^als/manager(|/.*)$": "<rootDir>/libs/manager/src/$1",
      "^als/reports(|/.*)$": "<rootDir>/libs/reports/src/$1"
    }
