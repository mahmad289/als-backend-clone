<html lang='en' style='padding: 20px 0'>
  <head>
    <meta charset='utf-8' />
    <title>Closing Summary Brief Report</title>
  </head>
  <body
    style='
      position: relative;
      /* width: 21cm; */
      width: 1188px;
      margin: 0 auto;
      color: #001028;
      background: #ffffff;
      font-family: Arial, sans-serif;
      font-size: 12px;'
  >
    {{#each this}}
      <div style='padding: 18px;'>
        <div style='page-break-before: always;'>
          <div style='padding-bottom: 16px'>
            <table style='width: 100%; table-layout: fixed'>
              <tbody>
                <tr style=''>
                  <td style='vertical-align: top' colspan='3'>
                    <h2 style='margin: 0 0 4px'>
                      {{client_name}}
                    </h2>
                  </td>

                  <td style='vertical-align: top'>
                    <img
                      src='data:image/png;base64,'
                      alt='img'
                      style='width: 100%'
                    />
                  </td>
                </tr>
              </tbody>
            </table>
          </div>
          <div style='margin-bottom: 16px'>
            <table
              style='
              width: 100%;
              text-align: left;
              font-size: 12px;
              border-spacing: 0;
              border: 1px solid black;
            '
            >
              <thead>
                <tr>
                  <th style='padding: 6px 8px; border-bottom: 1px solid black'>
                    Status as of
                  </th>
                  <th style='padding: 6px 8px; border-bottom: 1px solid black'>
                    {{status_as_of}}
                  </th>
                  <th
                    style='padding: 6px 8px; border-bottom: 1px solid black'
                    colspan='5'
                  >
                    Exp. Date
                  </th>
                </tr>
              </thead>
              <thead>
                <tr>
                  <th
                    style='
                    padding: 6px 8px;
                    border-bottom: 1px solid black;
                    border-right: 1px solid black;
                    background: lightblue;
                  '
                  >
                    Project Name
                  </th>
                  <th
                    style='
                    padding: 6px 8px;
                    border-bottom: 1px solid black;
                    border-right: 1px solid black;
                    background: lightblue;
                  '
                  >
                    Est Closing Date
                  </th>
                  <th
                    style='
                    padding: 6px 8px;
                    border-bottom: 1px solid black;
                    border-right: 1px solid black;
                    background: lightblue;
                  '
                  >
                    Material Documents
                  </th>
                  <th
                    style='
                    padding: 6px 8px;
                    border-bottom: 1px solid black;
                    border-right: 1px solid black;
                    background: lightblue;
                  '
                  ></th>
                  <th
                    style='
                    padding: 6px 8px;
                    border-bottom: 1px solid black;
                    border-right: 1px solid black;
                    background: lightblue;
                  '
                  >
                    Certificates and Supporting Info
                  </th>
                  <th
                    style='
                    padding: 6px 8px;
                    border-bottom: 1px solid black;
                    border-right: 1px solid black;
                    background: lightblue;
                  '
                  ></th>
                  <th
                    style='
                    padding: 6px 8px;
                    border-bottom: 1px solid black;
                    background: lightblue;
                  '
                  >
                    Current Status - Comments
                  </th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <td
                    style='
                    vertical-align: top;
                    padding: 0;
                    border-right: 1px solid black;
                  '
                  >
                    <div
                      style='
                      font-weight: bold;
                      padding: 6px 8px;
                      background: yellow;
                      border-bottom: 1px solid black;
                    '
                    >
                      {{project_details.name}}
                    </div>
                    <div style='margin-top: 30px'>
                      <div style='padding: 6px 8px; margin-bottom: 2px'>
                        <div style='font-weight: bold; margin-bottom: 4px'>
                          <span
                            style='text-decoration: underline'
                          >Investor</span>
                        </div>
                        {{project_details.investor}}
                      </div>
                      <div style='padding: 6px 8px; margin-bottom: 2px'>
                        <div style='font-weight: bold; margin-bottom: 4px'>
                          <span style='text-decoration: underline'>HHC Engineer</span>
                        </div>
                        {{project_details.hhc_engineer}}
                      </div>
                      <div style='padding: 6px 8px; margin-bottom: 2px'>
                        <div style='font-weight: bold; margin-bottom: 4px'>
                          <span style='text-decoration: underline'>ALS Analyst</span>
                        </div>
                        {{project_details.als_analyst}}
                      </div>
                      <div style='padding: 6px 8px; margin-bottom: 2px'>
                        <div style='font-weight: bold; margin-bottom: 4px'>
                          <span style='text-decoration: underline'>General
                            Partner</span>
                        </div>
                        {{project_details.general_partner}}
                      </div>
                      <div style='padding: 6px 8px; margin-bottom: 2px'>
                        <div style='font-weight: bold; margin-bottom: 4px'>
                          <span style='text-decoration: underline'>General
                            Contractor</span>
                        </div>
                        {{project_details.general_contractor}}
                      </div>
                      {{! <div style="padding: 6px 8px; margin-bottom: 2px">
                      <div style="font-weight: bold; margin-bottom: 4px">
                        <span style="text-decoration: underline">Investor</span>
                      </div>
                      Lettire Construction Corp
                    </div> }}
                    </div>
                  </td>
                  <td
                    style='
                    vertical-align: top;
                    padding: 0;
                    border-right: 1px solid black;
                  '
                  >
                    <div
                      style='padding: 6px 8px; border-bottom: 1px solid black'
                    >
                      {{project_details.closing_date}}
                    </div>
                  </td>
                  <td
                    style='
                    vertical-align: top;
                    padding: 0;
                    border-right: 1px solid black;
                  '
                  >
                    {{#each material_documents}}
                      <div
                        style='
                        font-weight: bold;
                        padding: 6px 8px;
                        height: 28px;
                        border-bottom: 1px solid black;
                      '
                      >
                        {{this.name}}:
                      </div>
                    {{/each}}
                  </td>
                  <td
                    style='
                    vertical-align: top;
                    width: 57px;
                    padding: 0;
                    border-right: 1px solid black;
                  '
                  >
                    {{#each material_documents}}
                      {{#ifEquals this.status 'G'}}
                        <div
                          style='
                        background: green;
                        padding: 6px 8px;
                        height: 28px;
                        border-bottom: 1px solid black;
                      '
                        ></div>
                      {{/ifEquals}}
                      {{#ifEquals this.status 'Y'}}
                        <div
                          style='
                        background: yellow;
                        padding: 6px 8px;
                        height: 28px;
                        border-bottom: 1px solid black;
                      '
                        ></div>
                      {{/ifEquals}}
                      {{#ifEquals this.status 'R'}}
                        <div
                          style='
                        background: red;
                        padding: 6px 8px;
                        height: 28px;
                        border-bottom: 1px solid black;
                      '
                        ></div>
                      {{/ifEquals}}
                    {{/each}}
                  </td>
                  <td
                    style='
                    vertical-align: top;
                    padding: 0;
                    border-right: 1px solid black;
                  '
                  >
                    {{#each certificates_and_supporting}}
                      <div
                        style='
                        font-weight: bold;
                        padding: 6px 8px;
                        height: 28px;
                        border-bottom: 1px solid black;
                      '
                      >
                        {{this.name}}
                      </div>
                    {{/each}}
                  </td>
                  <td
                    style='
                    vertical-align: top;
                    width: 57px;
                    padding: 0;
                    border-right: 1px solid black;
                  '
                  >
                    {{#each certificates_and_supporting}}
                      {{#ifEquals this.status 'G'}}
                        <div
                          style='
                        background: green;
                        padding: 6px 8px;
                        height: 28px;
                        border-bottom: 1px solid black;
                      '
                        ></div>
                      {{/ifEquals}}
                      {{#ifEquals this.status 'Y'}}
                        <div
                          style='
                        background: yellow;
                        padding: 6px 8px;
                        height: 28px;
                        border-bottom: 1px solid black;
                      '
                        ></div>
                      {{/ifEquals}}
                      {{#ifEquals this.status 'R'}}
                        <div
                          style='
                        background: red;
                        padding: 6px 8px;
                        height: 28px;
                        border-bottom: 1px solid black;
                      '
                        ></div>
                      {{/ifEquals}}
                    {{/each}}

                  </td>
                  <td style='width: 260px; padding: 0'>
                    <div style='padding: 6px 8px'>
                      {{#each comments}}
                        <br />{{this}}
                      {{/each}}
                    </div>
                  </td>
                </tr>
              </tbody>
            </table>
          </div>

          <div style='margin-bottom: 8px'>
            <table
              style='
              width: 100%;
              font-size: 12px;
              text-align: left;
              border-spacing: 0;
            '
            >
              <tbody>
                <tr>
                  <td>
                    <div style='font-weight: bold'>
                      Rehab or New Construction:
                      {{project_details.rehab_or_new_const}}
                    </div>
                  </td>
                </tr>
              </tbody>
            </table>
          </div>
          <div style='margin-bottom: 16px'>
            <table
              style='
              width: 100%;
              font-size: 12px;
              text-align: left;
              border-spacing: 0;
            '
            >
              <tbody>
                <tr>
                  <td>
                    <span style='font-weight: bold; margin-right: 8px'>Hard
                      Costs $:</span>{{project_details.hard_cost}}
                  </td>
                  <td>
                    <span style='font-weight: bold; margin-right: 8px'>Soft
                      Costs $:</span>{{project_details.soft_cost}}
                  </td>
                  <td>
                    <span style='font-weight: bold; margin-right: 8px'>Building
                      RCV:</span>{{project_details.bldg_rcv}}
                  </td>
                  <td>
                    <span style='font-weight: bold; margin-right: 8px'>Loss
                      Rents (12-mo):</span>{{project_details.loss_rents_12mo}}
                  </td>
                </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    {{/each}}
  </body>
</html>